{
  "extends": "nss/react",
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "no-alert": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "__schema"
        ]
      }
    ]
  }
}
